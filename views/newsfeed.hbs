                        <!-- start of terminal report section -->
                        <div class="tab-pane fade" id="ex3-tabs-2" role="tabpanel" aria-labelledby="ex3-tab-2">
                            <div>
                                    <div class="container">  
                                        <marquee data-aos="fade-up" data-aos-duration="1000" data-aos-delay="500" class="h5 text-shadow-lg text-primary " style="font-family: 'Open Sans', sans-serif;"> <a> Thank you for visiting my website and reading my blog. My goal is to provide you with valuable information and insights on a variety of topics related to Techonology</a></marquee>
                                      <div class="row">
                                        {{#each newsData}}
                                          <div class="col-xm-12 col-sm-6 col-md-4">
                                            <div class="card h-80 m-2">
                                              <a href="#">
                                                <img class="card-img-top" src="{{this.urlToImage}}" alt="{{this.title}}">
                                              </a>
                                              <div class="card-body"  style="font-family: 'Open Sans', sans-serif;">
                                                <h4 class="card-title"><a href={{this.url}}>{{this.title}}</a></h4>
                                                <p class="card-text">{{this.description}}</p>
                                              </div>
                                              <div class="card-footer">
                                                <small class="text-muted">{{this.publishedAt}}</small>
                                              </div>
                                          </div>
                                        </div>
                                        {{/each}}
                                      </div>
                                      </div>

                            </div>
                        <!-- end of terminal report sections -->
                    </div>
                        <div class="tab-pane fade" id="ex3-tabs-3" role="tabpanel" aria-labelledby="ex3-tab-3 text-justify">

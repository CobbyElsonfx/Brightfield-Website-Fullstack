                <div id="firstSectionInOtherDetails">
                    <ul class="nav nav-tabs nav-justified mb-3" id="ex1" role="tablist">
                        <li class="nav-item" role="presentation">
                        <a class="nav-link active" id="ex3-tab-1" data-mdb-toggle="tab" href="#ex3-tabs-1" role="tab" aria-controls="ex3-tabs-1" aria-selected="true">Insider</a>
                        </li>

                        <li class="nav-item" role="presentation">
                        <a class="nav-link" id="ex3-tab-2"  data-mdb-toggle="tab" href="#ex3-tabs-2" role="tab" aria-controls="ex3-tabs-2" aria-selected="false">TECH TALK</a>
                        </li>

                        <li class="nav-item" role="presentation">
                        <a class="nav-link" id="ex3-tab-3" data-mdb-toggle="tab" href="#ex3-tabs-3" role="tab" aria-controls="ex3-tabs-3" aria-selected="false" >CODE CULTURE</a>
                        </li>
                    </ul>
                    <!-- Tabs navs -->
                    
                    <!-- Tabs content -->
                    <div class="tab-content" id="ex2-content">

                        <div class="tab-pane first-tab-pane fade show active" id="ex3-tabs-1" role="tabpanel" aria-labelledby="ex3-tab-1">
                              <!-- Button to trigger navbar -->
                                 <div class="trigger container rounded-4 w-80  shadow-lg" style="width:70%;s">
                                    <form class="text-center p-4 " role="button" data-bs-toggle="modal" data-bs-target="#addPostModal"  >
                                      <input class="rounded-pill me-2 form-control" type="search" placeholder="What's on your mind?" aria-label="Search">
                                    </form>                                
                                </div>
      

                            <!-- Navbar to add post -->
                        <div class="modal fade" id="addPostModal" tabindex="-1">
                            <div class="modal-dialog">
                              <div class="modal-content">
                                <div class="modal-header">
                                  <h5 class="modal-title">Create Post</h5>
                                  <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                </div>
                                <div class="modal-body">
                                  <form>
                                    <div class="mb-3">
                                      <label for="post-content" class="form-label">What's on your mind?</label>
                                      <textarea class="form-control" id="post-content" rows="3"></textarea>
                                    </div>
                                  </form>
                                </div>
                                <div class="modal-footer">
                                  <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                                  <button type="button" class="btn btn-primary">Post</button>
                                </div>
                              </div>
                            </div>
                          </div>

